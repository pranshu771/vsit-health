
<% include ./partials/header %>
<% results.forEach(function(food) { %>
   <li> </li>


<div class="ui main text container segment">
    <div class="ui huge header"><%= food["item_name"] %></div>
    <div class="ui top attached">
       <div class="item">
            <div class="content">
                <span><%= food["calories"] %></span>
            </div>
            <div class="extra">
                <a class="ui button inverted purple" href="/food/<%=food['food_id']%>">Read More</a>
            </div>
        </div>
    </div>
</div>

<% }) %>
<% include ./partials/footer %>